{
    "python.pythonPath": "/usr/bin/python3",
    "editor.minimap.enabled": true,
    "breadcrumbs.enabled": true,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "explorer.confirmDelete": false,
    "git.autofetch": true,
    "yaml.customTags": [],
    "go.formatTool": "goimports",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Material Theme High Contrast",
    "vsicons.dontShowNewVersionMessage": true,
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.languageServer": "Pylance",
    "php.executablePath": "/usr/bin/php7.4",
    "python.showStartPage": false,
    "git.ignoreMissingGitWarning": true,
    "workbench.colorCustomizations": {},
    "liveServer.settings.donotShowInfoMsg": true,
    "git.enableSmartCommit": true,
    "powershell.powerShellDefaultVersion": "Windows PowerShell (x86)",
    "workbench.startupEditor": "welcomePage",
    "files.associations": {
        "*.html": "html",
        "*.php": "php"
    },
    "liveServer.settings.donotVerifyTags": true,
    "git.confirmSync": false,
    "C_Cpp.updateChannel": "Insiders",
    "debug.onTaskErrors": "showErrors",
    "explorer.confirmDragAndDrop": false,
    "terminal.integrated.shell.linux": "/bin/zsh",
    "jupyter.alwaysTrustNotebooks": true,
    "jupyter.askForKernelRestart": false,
    "mssql.connections": [
        {
            "server": "{{put-server-name-here}}",
            "database": "{{put-database-name-here}}",
            "user": "{{put-username-here}}",
            "password": ""
        },
        {
            "server": ".",
            "database": "",
            "authenticationType": "Integrated",
            "password": ""
        }
    ],
    "editor.linkedEditing": true,
    "workbench.editor.enablePreview": false,
    "[html]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "go.toolsManagement.autoUpdate": true,
    "files.autoSave": "afterDelay",
    "tabnine.experimentalAutoImports": true,
    "settingsSync.ignoredExtensions": [
        "bradlc.vscode-tailwindcss"
    ],
    "workbench.productIconTheme": "fluent-icons",
    "liveshare.presence": false,
    "files.maxMemoryForLargeFilesMB": 8000,
    "[css]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[makefile]": {
        "editor.insertSpaces": false
    },
    "discord.detailsIdling": "Coding Potato",
    "discord.largeImageIdling": "Coding Potato",
    "discord.lowerDetailsIdling": "Coding Potato",
    "[javascript]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "cmake.configureOnOpen": false,
    "gitlens.advanced.messages": {
        "suppressCreatePullRequestPrompt": true,
        "suppressLineUncommittedWarning": true
    },
    "sqltools.useNodeRuntime": true,
    "workbench.editor.untitled.hint": "hidden",
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "redhat.telemetry.enabled": true,
    "terminal.integrated.tabs.enabled": true,
    "settingsSync.ignoredSettings": [],
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.codeActionsOnSave": null,
    "cSpell.userWords": [
        "Codice",
        "Fiscale",
        "Singlehotel",
        "dbrouter",
        "iprestrict",
        "qrcode"
    ]
}